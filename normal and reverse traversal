#include<iostream>
#include<vector>
using namespace std;
int main(){
    int n=3;
    vector<int> nums(n);
    for(int i=0;i<n;i++){
        cin>>nums[i];
    }
     for(int i=0;i<n;i++){
        cout<<"Nomral "<<nums[i]<<" ";
    }
    cout<<endl;
    
    for(int i=n-1;i>=0;i--){
        cout<<"Reverse "<<nums[i]<<" ";
    }
    return 0;
}
