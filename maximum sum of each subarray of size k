#include<iostream>
using namespace std;
int main(){
    int n;
    int k;
    cin>>n>>k;
    
    int arr[n];
    
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    for(int i=0;i<=n-k;i++){
        int currentSum=0;
        for(int j=i;j<i+k;j++){
            currentSum+=arr[j];
        }
        cout<<"The maximum sum of each subarray of size k is "<<currentSum<<endl;
    }
    return 0;
}
