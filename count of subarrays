#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    
    int arr[n];
    
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }                                             // we can also calculate the number of subarray using the math equation 
                                                  // count = n(n+1)/2
    int count=0;
    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
             
                count++;
        }
    }
    cout<<count;
    return 0;
}
